 file = {Full Text:C\:\\Users\\dpolo\\Zotero\\storage\\2JVD3RC8\\Ntafalias et al. - 2022 - Design and Implementation of an Interoperable Arch.pdf:application/pdf},
 journal = {Smart Cities},
 keywords = {artificial intelligence, energy efficiency, interoperability, building integration, legacy equipment, smart buildings},
