 journal = {Sensors (Switzerland)},
 keywords = {Artificial intelligence, Blockchain, Cloud computing, Cybersecurity, Data processing, Communication systems, Cyber physical systems, Encryption, Internet-of-energy, Smart grid},
 number = {1},
 pages = {1--32},
 shorttitle = {Proof-of-puf enabled blockchain},
 title = {Proof-of-puf enabled blockchain: {Concurrent} data and device security for internet-of-energy},
