 issn = {2073-431X},
 journal = {Computers},
 keywords = {fog computing, edge computing, blockchain-enabled IoT (BIoT), internet of underwater things (IoUT), marine blockchain solutions, marine internet of things (MIoT)},
 language = {en},
 month = {December},
 number = {12},
 pages = {329},
